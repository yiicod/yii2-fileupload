    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Orlov",
            "email": "aaorlov88@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Virchenko Maksim",
            "email": "muslim1992@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "2.*",
        "blueimp/jquery-file-upload": "9.*",
        "yiicod/yii2-base": "1.*",
        "symfony/filesystem": "~3.0|~4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.1"
    },
    "repositories": [
        {
